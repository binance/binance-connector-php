{
    "name": "binance/derivatives-trading-portfolio-margin-pro",
    "version": "1.0.0",
    "description": "A thin layer for Binance API in PHP",
    "homepage": "https://github.com/binance/binance-connector-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Binance",
            "email": "contact@binance.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "binance/common": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Binance\\Client\\DerivativesTradingPortfolioMarginPro\\" : "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Binance\\Client\\DerivativesTradingPortfolioMarginPro\\Test\\" : "test/" }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../common"
        }
    ]
}
